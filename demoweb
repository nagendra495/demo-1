    <script>
        // Surprise Generator
        document.getElementById('generateSurprise').addEventListener('click', function() {
            const surpriseContent = document.getElementById('surpriseContent');
            const loveMessage = document.getElementById('loveMessage');
            const movieDialogue = document.getElementById('movieDialogue');
            
            // Array of romantic messages
            const messages = [
                "You are the source of my joy, the center of my world, and the whole of my heart!",
                "In your smile I see something more beautiful than the stars.",
                "Every love story is beautiful, but ours is my favorite.",
                "I love you not only for what you are, but for what I am when I am with you.",
                "You're the missing piece I never knew I was looking for."
            ];
            
            // Array of Telugu movie dialogues
            const dialogues = [
                '"Nuvvu naa hrudayam lo oka patam, nee prema tho rasanu... Nee pere thappa em undadu aa patam lo!"',
                '"Nee ishtam neeku teliyali, nee abhimanam naku teliyali... Idi oka relationship kaadu, idi oka love."',
                '"Manishi ki oka istam undali, oka korika undali... Naa korika neeve, naa istam neeve."',
                '"Nuvvu naaku chala avasaram... Nee lekunte nenu enni rojulu brathikagalanu, kani brathikanantha sukham ga brathikaleru."',
                '"Nuvvu naa daggaraki vacchesi, naa jeevithanni marchipoyanu... Ippudu nuvvu velthe, nenu em chestanu?"'
            ];
            
            // Randomly select a message and dialogue
            const randomMessage = messages[Math.floor(Math.random() * messages.length)];
            const randomDialogue = dialogues[Math.floor(Math.random() * dialogues.length)];
            
            // Update the content
            loveMessage.textContent = randomMessage;
            movieDialogue.textContent = randomDialogue;
            
            // Show the surprise content with animation
            surpriseContent.classList.add('active');
            
            // Change button text
            this.textContent = 'Generate Another Surprise';
        });

        // Smooth scrolling for navigation links
        document.querySelectorAll('nav a').forEach(anchor => {
            anchor.addEventListener('click', function(e) {
                e.preventDefault();
                const targetId = this.getAttribute('href');
                document.querySelector(targetId).scrollIntoView({
                    behavior: 'smooth'
                });
            });
        });

        // Add animation to cards on scroll
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.opacity = '1';
                    entry.target.style.transform = 'translateY(0)';
                }
            });
        }, observerOptions);

        // Observe cards
        document.querySelectorAll('.joke-card, .dialogue-card, .quote-card').forEach(card => {
            card.style.opacity = '0';
            card.style.transform = 'translateY(20px)';
            card.style.transition = 'opacity 0.5s ease, transform 0.5s ease';
            observer.observe(card);
        });
    </script>
